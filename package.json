{
  "name": "freelancer",
  "version": "0.0.1",
  "description": "A simple website for freelancers providing a homepage and payments pages to securely receive payments for services",
  "private": true,
  "dependencies": {
    "async": "^1.5.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "csurf": "^1.9.0",
    "debug": "^2.6.9",
    "ejs": "^2.5.5",
    "express": "^4.16.0",
    "helmet": "^3.12.1",
    "morgan": "~1.3.0",
    "nodemailer": "^4.6.7",
    "nodemailer-mailgun-transport": "^1.4.0",
    "response-time": "^2.3.2",
    "server-error-pages": "0.0.4",
    "stripe": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.5",
    "supertest": "^1.0.1"
  },
  "scripts": {
    "start": "export NODE_ENV=${NODE_ENV=development}; if [ $NODE_ENV = 'production' ]; then npm run start:prod; else npm run start:dev; fi",
    "start:dev": ". ./.env.dev; nodemon $NODE_DEBUG_OPTION ./bin/www",
    "start:prod": ". ./.env.prod; node $NODE_DEBUG_OPTION ./bin/www",
    "setup": "./bin/setup.sh"
  },
  "engines": {
    "node": "9.2.0"
  },
  "author": "Mark Smith"
}
